PROGRAM Hello
	BEGIN_VAR
		a: Integer;
		b: Integer;
		c: Integer;
	END_VAR
BEGIN
	a := 5;
	b := 3;
	c := 0;
	
	IF a = b THEN
		c := 0;
	END
	
	IF a <= b THEN
		c := 1;
	END
	
	IF a >= b THEN
		c := 2;
	END
	
	IF a != b THEN
		c := 3;
	END
	
	IF a < b THEN
		c := 4;
	END
	
	IF a > b THEN
		c := 5;
	END

	print(a);
	print(b);
	print(c);
END
